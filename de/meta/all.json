ator_sort_order":"05-03"},"16-3-1":{"composite_breakdown_label":"","computation_units":"H\u00e4ftlinge/100.000 Einwohner","copyright":"","data_footnote":"","data_non_statistical":false,"data_notice_class":"","data_notice_heading":"","data_notice_text":"","data_show_map":false,"data_start_values":[],"embedded_feature_footer":"","embedded_feature_html":"","embedded_feature_tab_title":"","embedded_feature_title":"","embedded_feature_url":"","expected_disaggregations":[],"graph_annotations":[],"graph_limits":[],"graph_stacked_disaggregation":"","graph_title":"Der Indikator gibt die Anzahl der Inhaftierten je 100.000 Einwohnerinnen und Einwohner an.","graph_titles":[],"graph_type":"line","indicator_name":"Inhaftierungsquote","indicator_number":"16.3.1","national_geographical_coverage":"","page_content":"","permalink":"","precision":[],"reporting_status":"complete","sort":"","standalone":false,"tags":[],"source_active_1":false,"source_organisation_1":"de-Quelle","source_url_1":"https://de-quelle-1.org","SDG_GOAL":null,"SDG_TARGET":null,"SDG_INDICATOR":null,"SDG_SERIES_DESCR":"","META_LAST_UPDATE":"","SDG_RELATED_INDICATORS":"","SDG_CUSTODIAN_AGENCIES":"","CONTACT_ORGANISATION":"","CONTACT_NAME":"","ORGANISATION_UNIT":"","CONTACT_FUNCT":"","CONTACT_PHONE":"","CONTACT_MAIL":"","CONTACT_EMAIL":"","STAT_CONC_DEF":"Ziel ist es, angesichts einer geringen Inhaftierungsquote in Schleswig-Holstein zuk\u00fcnftig einen Wert  von unter 40,0 zu erreichen. Die Bewertung bezieht sich auf die Daten (i.d.R. 2005 -2017) des 1. Berichts  Schleswig-Holsteins zu den UN-Nachhaltigkeitszielen. Trotz leichten Anstiegs befindet sich  Schleswig-Holstein europa- und, soweit verl\u00e4ssliche Vergleichsdaten vorliegen, weltweit unter den L\u00e4nder mit den geringsten Gefangenenquoten. Der Norden setzt das juristische Prinzip wirksam um, Freiheitsentzug  nur als letztes Mittel anzuwenden, was nicht zuletzt kriminologischen Erkenntnissen entspricht. Es ist zu  erwarten, dass die derzeit niedrige Inhaftierungsquote stabil bleiben und mittelfristig die Zielmarke von unter 40,0 erreicht werden wird.","UNIT_MEASURE":"","CLASS_SYSTEM":"","SOURCE_TYPE":"","COLL_METHOD":"","FREQ_COLL":"","REL_CAL_POLICY":"","DATA_SOURCE":"","COMPILING_ORG":"","INST_MANDATE":"","RATIONALE":"","REC_USE_LIM":"","DATA_COMP":"","DATA_VALIDATION":"","ADJUSTMENT":"","IMPUTATION":"","REG_AGG":"","DOC_METHOD":"","QUALITY_MGMNT":"","QUALITY_ASSURE":"","QUALITY_ASSMNT":"","COVERAGE":"","COMPARABILITY":"","OTHER_DOC":"","national_metadata_updated_date":"2024-09-02","national_data_updated_date":"2024-09-02","target_number":"16.3","goal_number":"16","indicator_sort_order":"16-03-01"},"2-2-4":{"indicator_name":"Test 2 2 4","indicator_number":"2-2-4","computation_units":null,"source_active_1":true,"source_organisation_1":"Quelle","source_url_1":"www.quelle.de","graph_title":"Test","graph_type":"bar","reporting_status":"complete","national_metadata_updated_date":"2024-11-28","national_data_updated_date":"2024-11-28","target_number":"2.2","goal_number":"2","data_non_statistical":false,"indicator_sort_order":"02-02-04"},"3-1-1":{"composite_breakdown_label":"","computation_units":"Anzahl der Punkte","copyright":"COPYRIGHT MAZUP","data_footnote":"","data_non_statistical":false,"data_notice_class":"","data_notice_heading":"","data_notice_text":"","data_show_map":false,"data_start_values":[],"embedded_feature_footer":"","embedded_feature_html":"","embedded_feature_tab_title":"","embedded_feature_title":"","embedded_feature_url":"","expected_disaggregations":[],"graph_annotations":[],"graph_limits":[],"graph_stacked_disaggregation":"","graph_title":"Name der Grafik im meta-Ordner","graph_titles":[],"graph_type":"line","indicator_name":"Name des de-Indikators","indicator_number":"3.1.1","national_geographical_coverage":"","page_content":"","permalink":"","precision":[],"reporting_status":"complete","sort":"","standalone":false,"tags":[],"source_active_1":true,"source_organisation_1":"de-Quelle","source_url_1